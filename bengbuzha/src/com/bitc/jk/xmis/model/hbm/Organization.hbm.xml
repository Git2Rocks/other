			<key column="公司ID"></key>
			<many-to-many column="部门ID" class="DepartmentObj"></many-to-many>
		</bag>
	</class>

	<class name="SimpleDeptObj" table="org_部门">
		<id name="departmentID" column="部门ID">
			<generator class="assigned" />
		</id>
